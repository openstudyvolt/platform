{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/react-starter-kit",
    "type": "project",
    "description": "AI-powered learning platform that helps students transform study materials into summaries, structured notes, and personalized learning paths for more efficient revision.",
    "keywords": [
        "AI-Powered Learning",
        "Academic Summarization",
        "Study Notes Generation",
        "Personalized Learning",
        "Gamification",
        "Virtual Study Rooms",
        "Adaptive Quiz",
        "Learning Analytics",
        "Student Engagement",
        "Education Technology",
        "EdTech",
        "AI Study Assistant",
        "Multilingual Support",
        "Offline Access",
        "Knowledge Retention",
        "Resource Recommendation",
        "Collaboration Tools",
        "Study Progress Tracking",
        "Goal-Based Learning",
        "Academic Productivity",
        "Content Moderation",
        "Security and Privacy",
        "Role-Based Access",
        "Study Companion",
        "OpenStudyVolt",
        "MVP Scope",
        "Agile Development",
        "Learning Automation",
        "Smart Study Platform",
        "Academic Efficiency"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "inertiajs/inertia-laravel": "^2.0",
        "laravel/framework": "^12.0",
        "laravel/socialite": "^5.21",
        "laravel/tinker": "^2.10.1",
        "spatie/laravel-activitylog": "^4.10",
        "tightenco/ziggy": "^2.4",
        "laravel/passport": "^13.0",
        "spatie/laravel-permission": "^6.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.18",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "OpenStudyVolt\\Auth\\": "packages/auth",
            "OpenStudyVolt\\Logic\\": "packages/logic",
            "OpenStudyVolt\\Support\\": "packages/support",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "packages/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "dev:ssr": [
            "npm run build:ssr",
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"php artisan inertia:start-ssr\" --names=server,queue,logs,ssr --kill-others"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
